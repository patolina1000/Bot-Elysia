{
  "name": "telegram-multi-bot-service",
  "version": "1.0.0",
  "description": "Multi-bot Telegram service with admin APIs and funnel analytics",
  "main": "dist/server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20 <26"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "migrate": "tsx src/db/runMigrations.ts",
    "verify:downsells": "tsx scripts/verify-downsells-schema.ts",
    "jobs:campaigns": "tsx src/jobs/runCampaigns.ts",
    "jobs:housekeeping": "tsx src/jobs/housekeeping.ts",
    "check:core-start": "tsx scripts/check-core-start.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "grammy",
    "multi-bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "grammy": "^1.21.1",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "undici": "^7.16.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.20",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
